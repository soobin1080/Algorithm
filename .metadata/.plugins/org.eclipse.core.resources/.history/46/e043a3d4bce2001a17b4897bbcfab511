import java.util.ArrayList;

public class pg_가장큰수 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] numbers= {6,10,2};
		
		ArrayList<Integer>[] arr=new ArrayList[9];
		
		for (int i = 0; i < numbers.length; i++) {
			if(numbers[i]>=1000) {
				int q=numbers[i]/1000;
			}
			
			if(numbers[i]>=100) {
				int q=numbers[i]/100;
			}
			
			if(numbers[i]>=10) {
				int q=numbers[i]/10;
			}
		}
		
		
		
	}

}
