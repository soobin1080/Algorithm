		for (int tc = 0; tc < TC; tc++) {
			//입력받기
			int r=sc.nextInt();
			int c=sc.nextInt();
			int cabbage=sc.nextInt();
			
			int arr[][]=new int[r][c];
			int vistied[][]=new int[r][c];
			
			for (int i = 0; i < cabbage; i++) {
				int y=sc.nextInt();
				int x=sc.nextInt();
				
				arr[y][x]=1;
			}
			
			
			
			
		}
	}

}
