import java.util.Scanner;

public class level1_solution1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		int answer = 0;

		if (n > 2) {

			for (int i = 3; i < n + 1; i++) {
				boolean flag = false;
				for (int j = 2; j < i; j++) {
					int mod = i % j;
					if (mod == 0) {
						flag = true;
						break;
					}
				}
				if (flag == false)
					answer++;
			}
			// 소수 2 포함
			answer++;
		} else if (n == 2)
			answer = 1;
		
		System.out.println(answer);
	}

}
