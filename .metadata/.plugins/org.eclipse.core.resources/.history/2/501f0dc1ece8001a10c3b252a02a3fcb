package codingtest;

public class solution2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="abcdcba";
		
		for (int i = 0; i < s.length(); i++) {//시작점
			for (int j = 1; j < s.length()+1; j++) {//몇개
				String a=s.substring(i,i+j);
			}
		}
				
				
	}

}
