
public class solution1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] answer = {};
		long n = 12345;
		String s = String.valueOf(n);
		
		char[] number=s.toCharArray();
		
		for (int i = 0; i < n; i++) {
			answer[i]=(number[n-i]-'0')
		}
	}

}
